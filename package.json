{
  "name": "banka_v2",
  "version": "2.0.0",
  "description": "A light-weight banking application",
  "main": "api/app.js",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run migrate:undo && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --require @babel/register mocha --timeout 20000 test/*.js --exit",
    "dev": "nodemon --exec babel-node ./api/app.js",
    "start": "node build/app.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "reset:db": "npm run migrate:undo && npm run migrate && npm run seed",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-coverage": "nyc report --reporter=text-lcov | codeclimate-test-reporter"
  },
  "author": "@ocranbillions",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.7.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "swagger-ui-express": "^4.0.2"
  }
}
